      "path": "registry/supremetoolkit/support-ticket-system/components/ui/CreateTicketForm.tsx",
      "target": "components/ui/create-ticket-form.tsx",
      "type": "registry:component"
    },
    {
      "path": "registry/supremetoolkit/support-ticket-system/components/ui/TicketList.tsx",
      "target": "components/ui/ticket-list.tsx",
      "type": "registry:component"
    },
    {
      "path": "registry/supremetoolkit/support-ticket-system/components/ui/TicketListItem.tsx",
      "target": "components/ui/ticket-list-item.tsx",
      "type": "registry:component"
    },
    {
      "path": "registry/supremetoolkit/support-ticket-system/components/ui/TicketView.tsx",
      "target": "components/ui/ticket-view.tsx",
      "type": "registry:component"
    },
    {
      "path": "registry/supremetoolkit/support-ticket-system/components/admin/AdminTicketDashboard.tsx",
      "target": "components/admin/admin-ticket-dashboard.tsx",
